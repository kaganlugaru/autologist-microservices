# Environment variables для Autologist Backend
# Скопируйте этот файл в .env и заполните свои значения

# === SUPABASE НАСТРОЙКИ ===
# Получите эти значения из Settings > API в вашем Supabase проекте
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# === СЕРВЕР ===
PORT=3000
NODE_ENV=development

# === TELEGRAM (для парсера) ===
TELEGRAM_API_ID=your-api-id
TELEGRAM_API_HASH=your-api-hash
TELEGRAM_SESSION_STRING=your-session
TELEGRAM_NOTIFY_USER_ID=your-user-id

# === ИИ API КЛЮЧИ ===
DEEPSEEK_API_KEY=your-deepseek-key
OPENAI_API_KEY=your-openai-key
GEMINI_API_KEY=your-gemini-key